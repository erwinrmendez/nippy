{
  "name": "nippy",
  "description": "Chrome app for notes and code snippets.",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "nippy",
    "default_popup": "index.html"
  }
}
